{
    "type of run": "real run",
    "dataset": {
        "file name": "m4_daily_finance.json",
        "file type": "json",
        "file path": "datasets/",
        "cumulate used":30,
        "use series": 50,
        "min length": 1000,
        "max length": 1500
    },
    "transformation config": {
        "xthresholds": [0.01, 1.1, 0.1],
        "interp kind": ["linear"],
        "use states": [true]
    },
    "modelling config": {
        "retrain window":10,
        "validation size": 0.1,
        "test size": 0.1,
        "forecast horizon": 1,
        "gap": 0,
        "score measure": "SMAPE"
    },
    "models": ["EN", "MLP", "LSVR", "RF", "LGBM"],
    "execution config": {
        "n of workers": 24
    }
}

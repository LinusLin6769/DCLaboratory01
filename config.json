{
    "type of run": "real run",
    "dataset": {
        "file name": "m4_hourly_dataset.json",
        "file type": "json",
        "file path": "datasets/",
        "cumulate used": 0,
        "use series": 20,
        "min length": 1000,
        "max length": 1500
    },
    "transformation config": {
        "xthresholds": [0.01, 2.1, 0.2],
        "interp kind": ["linear"],
        "use states": [true],
        "target transformation": "log-return"
    },
    "modelling config": {
        "retrain window": 10,
        "validation size": 0.1,
        "test size": 0.1,
        "forecast horizon": 1,
        "gap": 0,
        "score measure": "SMAPE",
        "task": "univariate forecasting"
    },
    "models": ["MA", "EN", "ETS", "MLP", "LSVR", "RF"],
    "execution config": {
        "n of workers": 22
    }
}

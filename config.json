    "modelling config": {
        "retrain window":10,
        "validation size": 0.1,
        "test size": 0.1,
        "forecast horizon": 1,
        "gap": 0,
        "score measure": "SMAPE"
    },
    "models": ["ETS", "LGBM"],
    "execution config": {
        "n of workers": 22
    }
}
